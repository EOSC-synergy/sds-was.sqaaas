<model xmlns:i18n="http://xml.zope.org/namespaces/i18n" xmlns:marshal="http://namespaces.plone.org/supermodel/marshal" xmlns:form="http://namespaces.plone.org/supermodel/form" xmlns:security="http://namespaces.plone.org/supermodel/security" xmlns:users="http://namespaces.plone.org/supermodel/users" xmlns:lingua="http://namespaces.plone.org/supermodel/lingua"
xmlns="http://namespaces.plone.org/supermodel/schema">
  <schema>
    <field name="full_description" type="plone.app.textfield.RichText">
      <description/>
      <required>True</required>
      <title>Full description</title>
    </field>
    <field name="image" type="plone.namedfile.field.NamedBlobImage">
      <description/>
      <required>False</required>
      <title>Image</title>
    </field>
    <field name="venue" type="zope.schema.TextLine">
      <description/>
      <required>False</required>
      <title>Venue</title>

    </field>
    <field name="presented_by" type="zope.schema.TextLine">
      <description/>
      <required>True</required>
      <title>Presented by</title>
    </field>
    <field name="event_type" type="zope.schema.Choice">
      <description/>
      <required>False</required>
      <title>Event type</title>
      <values>
        <element>Workshop</element>
        <element>Training School</element>
        <element>Meeting</element>
      </values>
    </field>
    <field name="event_language" type="zope.schema.TextLine">
      <description/>
      <required>False</required>
      <title>Event Language</title>
    </field>
    <field name="website_url" type="zope.schema.URI">
      <description/>
      <required>False</required>
      <title>Website URL</title>
    </field>
    <field name="register_url" type="zope.schema.URI">
      <description/>
      <required>False</required>
      <title>Register website URL</title>
    </field>
    <field name="document" type="plone.namedfile.field.NamedBlobFile">
      <description/>
      <required>False</required>
      <title>Agenda</title>
    </field>
  </schema>
</model>