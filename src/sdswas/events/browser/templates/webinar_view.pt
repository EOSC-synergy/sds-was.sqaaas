<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="sdswas.events">
<body>
<metal:javascriptslot fill-slot="javascript_head_slot">

      <script type="text/javascript" tal:define="navroot context/@@plone_portal_state/navigation_root_url"
              tal:attributes="src string:${navroot}/++resource++sdswas.events/js/webinar_view.js?1633441656932"/>

</metal:javascriptslot>

<metal:main
      fill-slot="main"
      tal:define="is_upcoming view/is_upcoming;
      text_upcoming string: Upcoming Webinar;
      text_past string: Past Webinar;">


      <metal:macro use-macro="context/banner_title_macro/macros/master"
            tal:define="title python:text_upcoming if is_upcoming else text_past"
            />

            <!-- Wrapper to fix the max width -->
      <div id="view" class="main-wrap"
            tal:define = "upcoming_boolean string:upcoming"
            tal:attributes="upcoming python: upcoming_boolean if is_upcoming else None">

            <metal:macro
                  use-macro="context/events_utils/macros/breadcumb"/>

            <!-- Modal window main content ends -->
            <div class="modwin-dynamic-content">
                  <metal:macro use-macro="context/webinar_modal/macros/main-content"/>
            </div><!-- Modal window maincontent ends -->

      </div>  <!-- Wrapper to fix the max width ends-->

</metal:main>
</body>
</html>