<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="sdswas.events">
<body>
    <metal:main
            fill-slot="main"
            tal:define="is_upcoming view/is_upcoming;
            text_upcoming string: Upcoming Events;
            text_past string: Resources;
            page_title python:text_upcoming if is_upcoming else text_past">
            <metal:macro use-macro="context/events_utils/macros/page-title">
            <!-- Wrapper to fix the max width -->
            <div class="main-wrap">

                  <metal:macro use-macro="context/events_utils/macros/breadcumb"/>

                  <!-- Modal window main content ends -->
                  <div class="modwin-main-content bg-color-white">
                        <metal:macro use-macro="context/webinar_modal/macros/main-content"/>
                  </div><!-- Modal window maincontent ends -->

            </div>  <!-- Wrapper to fix the max width ends-->

     </metal:main>
</body>
</html>